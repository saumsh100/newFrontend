{
  "name": "carecru",
  "version": "1.91.0",
  "description": "Dental Practice Dashboard",
  "author": "Justin Sharp <justin@carecru.com>",
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.0"
  },
  "private": true,
  "scripts": {
    "client:dev:server": "webpack-dev-server --config ./webpack/client/webpack.development.config.js",
    "client:build:prod": "webpack -p --config ./webpack/client/webpack.production.config.js",
    "client:watch:widget": "webpack --config ./webpack/client/webpack.widget.config.js --watch",
    "server:watch": "webpack --watch --config ./webpack/server/webpack.config.js",
    "server:build:prod": "webpack --config ./webpack/server/webpack.config.js",
    "build:dist": "node server/bin/deploy.js",
    "console": "node server/bin/console.js",
    "seeds": "babel-node ./server/bin/seeds.js",
    "rebuild": "export $(cat .env | xargs) && sequelize db:migrate:undo:all && sequelize db:migrate",
    "rebuild:seed": "npm run rebuild && sequelize db:seed:all",
    "start": "nf start -w -j Procfile_dev",
    "startdev": "nf start -j Procfile_dev_no_reminders",
    "start:frontend:dev": "nf start -w -j Procfile",
    "start:backend:dev": "nodemon server/bin/server.js --no-webpack",
    "storybook": "start-storybook -s ./statics/fonts -p 6006",
    "test": "npm run test:api && npm run test:lib",
    "test:cypress": "cypress run",
    "test:jest": "export $(cat .env | xargs) && jest --verbose --forceExit",
    "test:api": "export $(cat .env | xargs) && jest tests/__tests__/api --verbose --runInBand --forceExit",
    "test:lib": "export $(cat .env | xargs) && jest tests/__tests__/lib --verbose --runInBand --forceExit",
    "test:graphql": "export $(cat .env | xargs) && jest tests/__tests__/graphql --verbose --runInBand --forceExit",
    "clean": "npm run clean:assets && npm run clean:modules && npm run clean:server:bundles",
    "clean:server:bundles": "rimraf server/bin/build",
    "clean:assets": "rimraf statics/assets",
    "clean:modules": "rimraf node_modules",
    "postinstall": "npm run server:build:prod && npm run update-schema && npm run relay && npm run client:build:prod",
    "remind:cron": "nf start -j REMIND_CRON",
    "build-storybook": "build-storybook",
    "electron:install": "npm --prefix ./electron install",
    "electron:dev": "NODE_ENV=development electron ./electron/",
    "electron:build:mac": "electron-builder build -m --projectDir='./electron' --config='electron-builder-dev.yaml'",
    "electron:build:win": "electron-builder build -w --projectDir='./electron' --config='electron-builder-dev.yaml'",
    "electron:build": "electron-builder build -mw --projectDir='./electron'",
    "electron:publish:dev": "export $(cat .env | xargs) && electron-builder build -mw --projectDir='./electron' --publish always --config='electron-builder-dev.yaml'",
    "electron:publish:prod": "export $(cat .env | xargs) && electron-builder build -mw --projectDir='./electron' --publish always --config='electron-builder-prod.yaml'",
    "electron:build:publish": "npm run client:build:prod && npm run electron:publish",
    "relay:watch": "npm run relay -- --watch",
    "relay": "relay-compiler --src ./client --schema server/graphql/data/schema.json --extensions js jsx",
    "update-schema": "node ./server/bin/build/updateSchema.bundle.js",
    "format": "prettier-eslint --eslint-config-path=.carecru.eslintrc --write",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "client/**/*.scss": [
      "npm run format",
      "git add"
    ],
    "client/**/*.{js,jsx}": [
      "npm run format",
      "jest",
      "git add"
    ]
  },
  "jest": {
    "projects": [
      {
        "testURL": "http://localhost/",
        "setupTestFrameworkScriptFile": "<rootDir>/tests/util/jestSetup.js",
        "displayName": "test",
        "moduleNameMapper": {
          "^CareCruModels$": "<rootDir>/server/_models/index.js",
          "^CareCruGraphQL/(.*)": "<rootDir>/server/graphql/$1"
        }
      },
      {
        "testURL": "http://localhost/",
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/client/**/*.{js,jsx}"
        ]
      }
    ]
  },
