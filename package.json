{
  "name": "carecru",
  "version": "2.97.0",
  "description": "Dental Practice Dashboard",
  "author": "Justin Sharp <justin@carecru.com>",
  "engines": {
    "node": "8.11.3",
    "npm": ">=6.5.0"
  },
  "private": true,
  "scripts": {
    "start:dev": "nf start -j Procfile",
    "start:web": "nf start -j ProcfileWeb",
    "start:commsJobs": "nf start -j ProcfileCommsJobs",
    "start:patientCache": "nf start -j ProcfilePatientCache",
    "npmrc": "dotenv -- node scripts/npmrcGenerate.js",
    "preinstall": "node scripts/npmrcCheck.js",
    "postinstall": "npm run server:build:prod",
    "client:build": "npm run graphl:build && npm run client:build:prod && npm run client:widget:build",
    "client:version": "cd client/build && npm version",
    "client:publish": "cd client/build && npm publish . --access private",
    "client:dev:server": "dotenv -- webpack-dev-server --progress --config ./client/webpack/webpack.development.config.js",
    "client:build:prod": "dotenv -- webpack -p --config ./client/webpack/webpack.production.config.js",
    "client:widget:watch": "npm run client:widget:build -- --watch --progress",
    "client:widget:build": "dotenv -- webpack --config ./client/webpack/webpack.widget.config.js",
    "relay:watch": "npm run relay -- --watch",
    "relay": "relay-compiler --src ./client --schema server/graphql/data/schema.json --extensions js jsx",
    "update-schema": "node ./server/bin/build/updateSchema.bundle.js",
    "graphl:build": "npm run update-schema && npm run relay",
    "server:watch": "npm run server:build:prod -- --watch --progress",
    "server:build:prod": "dotenv -- webpack --config ./server/webpack/webpack.config.js",
    "ngrok": "ngrok http localhost:5100",
    "twilio:watch": "twilio-run --live --port=5101",
    "twilio:ngrok": "twilio-run --live --port=5101 --ngrok",
    "clean": "npm run clean:assets && npm run clean:modules && npm run clean:server:bundles",
    "clean:server:bundles": "rimraf server/bin/build",
    "clean:assets": "rm $(find ./client/build -type f ! -name .gitignore ! -name package.json)",
    "clean:modules": "rimraf node_modules",
    "rebuild": "dotenv -- sequelize db:migrate:undo:all && sequelize db:migrate",
    "seed": "dotenv -- sequelize db:seed:all",
    "rebuild:seed": "npm run rebuild && npm run seed",
    "storybook": "start-storybook -s ./statics/fonts -p 6006",
    "test": "npm run test:api && npm run test:lib && npm run test:graphql",
    "test:watch": "dotenv -- jest --watch",
    "test:jest": "dotenv -- jest --verbose --forceExit",
    "test:api": "dotenv -- jest tests/__tests__/api --verbose --runInBand --forceExit",
    "test:lib": "dotenv -- jest tests/__tests__/lib --verbose --runInBand --forceExit",
    "test:graphql": "dotenv -- jest tests/__tests__/graphql --verbose --runInBand --forceExit",
    "build-storybook": "build-storybook",
    "electron:install": "npm --prefix ./electron install",
    "electron:dev": "NODE_ENV=development electron ./electron/",
    "electron:build:mac": "electron-builder build -m --projectDir='./electron' --config='electron-builder-dev.yaml'",
    "electron:build:win": "sh ./electron/pre_win_build.sh && electron-builder build -w --projectDir='./electron' --config='electron-builder-dev.yaml' && sh ./electron/post_win_build.sh",
    "electron:build": "electron-builder build -mw --projectDir='./electron'",
    "electron:publish:win:dev": "sh ./electron/pre_win_build.sh && export $(cat .env | xargs) && electron-builder build -w --projectDir='./electron' --publish always --config='electron-builder-dev.yaml' && sh ./electron/post_win_build.sh",
    "electron:publish:mac:dev": "export $(cat .env | xargs) && electron-builder build -m --projectDir='./electron' --publish always --config='electron-builder-dev.yaml'",
    "electron:publish:win:prod": "sh ./electron/pre_win_build.sh && export $(cat .env | xargs) && electron-builder build -w --projectDir='./electron' --publish always --config='electron-builder-prod.yaml' && sh ./electron/post_win_build.sh",
    "electron:publish:mac:prod": "export $(cat .env | xargs) && electron-builder build -m --projectDir='./electron' --publish always --config='electron-builder-prod.yaml'",
    "electron:publish:dev": "npm run electron:publish:win:dev && npm run electron:publish:mac:dev",
    "electron:publish:prod": "npm run electron:publish:win:prod && npm run electron:publish:mac:prod",
    "electron:build:publish": "npm run client:build:prod && npm run electron:publish:prod",
    "format": "prettier-eslint --eslint-config-path=.carecru.eslintrc --write",
    "run:job:reminders": "dotenv -- node ./server/bin/build/remindersJob.bundle.js",
    "run:job:recalls": "dotenv -- node ./server/bin/build/recallsJob.bundle.js",
    "run:job:reviews": "dotenv -- node ./server/bin/build/reviewsJob.bundle.js",
    "run:test:calls": "dotenv -- node ./server/bin/build/makeTestCall.bundle.js"
  },
